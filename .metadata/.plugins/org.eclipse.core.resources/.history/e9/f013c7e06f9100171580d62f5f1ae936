#include <stdio.h>
#include <string.h>

#include "../Concurrent/dictionary.h"

int main(int argc,char **argv)
{
		CLIENT *cl;
		in input;
		out output;
		if(argc!=2){
			printf("client localhost");
			exit(1);
		}

		cl = clnt_create(argv[1],DICTIONARY_PROG,DICTIONARY_VERS,"tcp");
		printf("Enter the word : ");
		scanf("%s",input.word);
		insert_proc_1(&input,&output,cl);
		printf("%s",output->mean);

}
