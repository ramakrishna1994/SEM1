int main()
{
	int sockfd,buf[256],portno;
	char mip[32];
	struct sockaddr_in mcast;
	printf("Enter Multicast IP address to Send Data :");
	scanf("%s",mip);
	printf("Enter Multicast Port : ");
	scanf("%d",&portno);
	sockfd = socket(AF_INET,SOCK_DGRAM,0);

}
