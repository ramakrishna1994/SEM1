#include <stdio.h>
#include <string.h>

#include "dictionary.h"

int main(int argc,char **argv)
{
		CLIENT *cl;
		input input;
		output output;
		if(argc!=2){
			printf("client localhost");
			exit(1);
		}

		cl = clnt_create(argv[1],DICTIONARY_PROG,DICTIONARY_VERS,"tcp");
		printf("Enter the word : ");
		scanf("%s",input.word);
		bool_t ba = insert_proc_1(&input,&output,cl);
		printf("%d",ba);
		printf("%s",output.mean);

}
