#include <stdio.h>
#include <string.h>

#include "dictionary.h"

int main(int argc,char **argv)
{
		CLIENT *cl;
		input in;
		output out;
		if(argc!=2){
			printf("client localhost");
			exit(1);
		}

		cl = clnt_create(argv[1],DICTIONARY_PROG,DICTIONARY_VERS,"tcp");
		printf("Enter the word : ");
		fgets(in.word, sizeof(in.word), stdin);
		printf("Enter the meaning :");
		scanf(" %s",in.meaning);
		bool_t ba = insert_proc_1(&in,&out,cl);
		printf("%d",out.status);

}
